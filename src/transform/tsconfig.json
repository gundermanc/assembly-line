{
    "compilerOptions": {
        "baseUrl": "..",
        "module": "ES2022",
        "moduleResolution": "Node",
        "noImplicitAny": true,
        "outDir": "../../dist",
        "paths": {
            "*": ["node_modules/*"]
        },
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": true,
        "strict": true,
        "target": "ESNext",
        "esModuleInterop": true,
        "allowJs": true
    },
    "files": [
        "transform.mjs",
        "transformLogic.ts"
    ]
}
